{
    "name": "Giant Floating Baby Head",
	"short_name": "GFBH",
    "description": "Enjoy a random Giant Floating Baby Head",
    "version": "0.6.6",
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "css": ["css/css.css"],
        "js": ["lib/jquery-2.1.1.min.js","js/script.js"]
    }],
